{
  "name": "blog",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite --port 3001",
    "deploy:dev": "pnpm build && pnpm run preview",
    "build": "tsc -b && vite build",
    "lint": "eslint .",
    "preview": "vite preview --port 3001",
    "dev:mfe": "rimraf dist && concurrently -k -n build,serve -c yellow,magenta \"vite build --watch --mode development\" \"wait-on dist/assets/remoteEntry.js && vite preview --port 3001 --strictPort\"",
    "build:ghp": "vite build --base=/my-blog/blog/"
  },
  "dependencies": {
    "@srf/ui": "workspace:*",
    "@mdx-js/mdx": "^3.1.0",
    "@mdx-js/react": "^3.1.0",
    "@mdx-js/runtime": "^2.0.0",
    "@mfe/shared": "workspace:*",
    "@tailwindcss/vite": "^4.1.12",
    "@tanstack/react-query": "^5.85.3",
    "@toast-ui/editor": "^3.2.2",
    "@toast-ui/react-editor": "^3.2.3",
    "highlight.js": "^11.11.1",
    "i": "^0.3.7",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "react-router-dom": "^7.8.1",
    "rehype-autolink-headings": "^7.1.0",
    "rehype-slug": "^6.0.0",
    "remark-gfm": "^4.0.1",
    "tailwindcss": "^4.1.11",
    "zustand": "^5.0.7"
  },
  "devDependencies": {
    "@eslint/js": "^9.33.0",
    "@mdx-js/rollup": "^3.1.0",
    "@originjs/vite-plugin-federation": "^1.4.1",
    "@types/react": "^19.1.10",
    "@types/react-dom": "^19.1.7",
    "@vitejs/plugin-react": "^5.0.0",
    "eslint": "^9.33.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "fast-glob": "^3.3.3",
    "globals": "^16.3.0",
    "gray-matter": "^4.0.3",
    "typescript": "~5.8.3",
    "typescript-eslint": "^8.39.1",
    "vite": "^7.1.2",
    "vite-plugin-federation": "^0.0.1",
    "vite-plugin-static-copy": "^3.1.2"
  }
