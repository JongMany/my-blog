{
  "name": "shell",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite --port 3000",
    "deploy:dev": "pnpm build && pnpm run preview",
    "build": "tsc -b && vite build",
    "lint": "eslint .",
    "preview": "vite preview --port 3000",
    "dev:mfe": "concurrently -k -n build,serve -c yellow,magenta \"vite build --watch --mode development\" \"vite preview --port 3000 --strictPort\"",
    "build:ghp": "pnpm -w -F @srf/tokens build && vite build --base=/my-blog/"
  },
  "dependencies": {
    "@mfe/shared": "workspace:*",
    "@radix-ui/themes": "^3.2.1",
    "@srf/tokens": "workspace:*",
    "@srf/ui": "workspace:*",
    "@srf/utils": "workspace:*",
    "@tailwindcss/vite": "^4.1.12",
    "@tanstack/react-form": "^1.19.2",
    "@tanstack/react-query": "^5.85.3",
    "@tanstack/react-query-devtools": "^5.85.3",
    "framer-motion": "^12.23.12",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "react-helmet-async": "^2.0.5",
    "react-router-dom": "^7.8.1",
    "tailwindcss": "^4.1.11",
    "zustand": "^5.0.7"
  },
  "devDependencies": {
    "@eslint/js": "^9.33.0",
    "@originjs/vite-plugin-federation": "^1.4.1",
    "@types/react": "^19.1.10",
    "@types/react-dom": "^19.1.7",
    "@vitejs/plugin-react": "^5.0.0",
    "eslint": "^9.33.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "globals": "^16.3.0",
    "typescript": "~5.8.3",
    "typescript-eslint": "^8.39.1",
    "vite": "^7.1.2"
  }
}
